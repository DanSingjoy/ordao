{
  "name": "@ordao/ts-utils",
  "version": "1.0.4",
  "type": "commonjs",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "to-dual-package": "tsconfig-to-dual-package tsconfig.esm.json tsconfig.cjs.json",
    "build": "npm run build:cjs && npm run build:esm && npm run to-dual-package",
    "clean": "rimraf dist",
    "build-clean": "npm run clean && npm run build",
    "prepack": "npm run build-clean"
  },
  "author": "sim31",
  "license": "GPL-3.0",
  "description": "",
  "files": [
    "/dist/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "devDependencies": {
    "tsconfig-to-dual-package": "^1.2.0",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sim31/ordao.git",
    "directory": "libs/ts-utils"
  }
}
