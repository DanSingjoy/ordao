{
  "name": "@ordao/ortypes",
  "version": "1.0.8",
  "description": "Types for Optimistic Respect Based DAO and its components",
  "type": "commonjs",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "to-dual-package": "tsconfig-to-dual-package tsconfig.esm.json tsconfig.cjs.json",
    "build": "npm run build:cjs && npm run build:esm && npm run to-dual-package",
    "clean": "rimraf dist",
    "build-clean": "npm run clean && npm run build",
    "prepack": "npm run build-clean"
  },
  "keywords": [
    "DAO"
  ],
  "author": "sim31",
  "license": "GPL-3.0",
  "files": [
    "/dist/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./orContext": {
      "require": {
        "types": "./dist/cjs/orContext.d.ts",
        "default": "./dist/cjs/orContext.js"
      },
      "import": {
        "types": "./dist/esm/orContext.d.ts",
        "default": "./dist/esm/orContext.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/cjs/*",
        "default": "./dist/cjs/*"
      },
      "import": {
        "types": "./dist/esm/*",
        "default": "./dist/esm/*"
      }
    }
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/chai": "^4.3.16",
    "typechain": "^8.3.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@ordao/ethers-decode-error": "^1.0.5",
    "@ordao/orec": "^1.0.8",
    "@ordao/respect1155": "^1.0.8",
    "@ordao/ts-utils": "^1.0.5",
    "chai": "^5.1.1",
    "op-fractal-sc": "github:Optimystics/op-fractal-sc#main",
    "tsconfig-to-dual-package": "^1.2.0",
    "utility-types": "^3.11.0"
  },
  "peerDependencies": {
    "ethers": "^6.13.0",
    "zod": "^3.23.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sim31/ordao.git",
    "directory": "libs/ortypes"
  }
}
