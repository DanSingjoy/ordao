{
  "compilerOptions": {
    "moduleResolution": "Node16",
    "module": "Node16",
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "rootDir": "./src",
    "jsx": "react-jsx",
  },
  "include": [
    "src/**/*"
  ]
}
